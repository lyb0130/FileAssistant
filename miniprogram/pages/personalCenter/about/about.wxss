.container {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: #e7eaef;
}
.logo {
  position: fixed;
  top: 80px;
  left: 50%;
  width: 100px;
  height: 100px;
  transform: translateX(-50%);
}
.title{
  position: fixed;
  top: 180px;
  left: 50%;
  width: 40px;
  height: 30px;
  transform: translateX(-50%);
  font-size: 20px;
  font-weight: bold;
}
.version{
  position: fixed;
  top: 205px;
  left: 50%;
  width: 100px;
  height: 40px;
  transform: translateX(-50%);
}
.table {
  position: relative;
  margin-top: 250px;
  /* margin-bottom: 10px; */
  margin-left: 50px;
  margin-right: 50px;
  height: 100px;
  background: #ffffff;
  border-radius: 25px;
  padding: 25px;
  border: 1px solid #36b4d8;
}
.content {
  margin: 0;
  text-indent: 2em;
  text-align: justify;
  line-height:25px;
  /* font-family:"微软雅黑"; */
  font-size: 16px;
}